{
  "name": "shortify-my-url",
  "version": "1.2.0",
  "description": "Simple URL shortener.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "postinstall": "tsc && npm run pack:static-files",
    "pack:static-files": "parcel build src/public/*.html -d dist/public --no-source-maps --no-cache",
    "tslint": "tslint --project tsconfig.json",
    "dev": "nodemon src/server.ts"
  },
  "keywords": [
    "express.js",
    "vue.js",
    "redis"
  ],
  "engines": {
    "node": "12.18.3"
  },
  "author": "Ivan Tachenko",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "^5.1.3",
    "joi": "17.2.1",
    "nanoid": "3.1.12",
    "pino": "^6.7.0",
    "redis": "3.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/joi": "^14.3.4",
    "@types/node": "^14.14.2",
    "@types/pino": "^6.3.3",
    "@types/redis": "^2.8.28",
    "axios": "^0.21.0",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "vue": "^2.6.12"
  }
}
